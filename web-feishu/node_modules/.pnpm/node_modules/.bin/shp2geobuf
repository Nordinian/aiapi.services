#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/geobuf@3.0.2/node_modules/geobuf/bin/node_modules:/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/geobuf@3.0.2/node_modules/geobuf/node_modules:/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/geobuf@3.0.2/node_modules:/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/geobuf@3.0.2/node_modules/geobuf/bin/node_modules:/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/geobuf@3.0.2/node_modules/geobuf/node_modules:/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/geobuf@3.0.2/node_modules:/Users/Kinson/aiapi.services/aiapi.services/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../geobuf/bin/shp2geobuf" "$@"
else
  exec node  "$basedir/../geobuf/bin/shp2geobuf" "$@"
fi
